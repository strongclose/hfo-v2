---
import Layout from '../layouts/BaseLayout.astro';
import { ProvidersIndexPage } from '../components/ProvidersIndexPage';
---

<Layout title="Healthcare Providers Directory - HealthFees.org">
  <ProvidersIndexPage
    client:load
    onNavigateToProviderDetails={(providerSlug) => window.location.href = `/providers/${providerSlug}`}
    onNavigateToDisclosures={() => window.location.href = "/disclosures"}
  />
</Layout>
